class TestNixonngetes():
  def setup_method(self, method):
    self.driver = webdriver.Chrome()
    self.vars = {}
  
  def teardown_method(self, method):
    self.driver.quit()
  
  def test_nixonngetes(self):
    self.driver.get("https://seleniumbase.io/demo_page")
    self.driver.set_window_size(980, 816)
    self.driver.find_element(By.ID, "mySelect").click()
    dropdown = self.driver.find_element(By.ID, "mySelect")
    dropdown.find_element(By.XPATH, "//option[. = 'Set to 50%']").click()
    self.driver.find_element(By.ID, "myButton").click()
    self.driver.find_element(By.ID, "radioButton2").click()
  
